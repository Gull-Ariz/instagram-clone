<div class='container top-margin'>
  <%= render 'shared/flashmessage' %>
  <%= form_with model: @story, local: true do |form|%>
    <div class='form-group'>
      <%= form.label :images%>
      <%= form.file_field :images, required: true, accept:'image/*', multiple: true, class:'form-control' %>
    </div>
    <div class='form-group'>
      <%= form.submit class: 'btn btn-success'%>
    </div>
  <% end%>
</div>
